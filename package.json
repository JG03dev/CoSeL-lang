{
  "name": "cosel-lang",
  "displayName": "CoSeL",
  "description": "Syntax Highlighting for CoSeL language",
  "version": "1.0.0",
  "publisher": "JG03dev",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "cosel",
      "aliases": ["CoSeL", "cosel"],
      "extensions": [".csm",".csl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "cosel",
      "scopeName": "source.csm, source.csl",
      "path": "./syntaxes/cosel.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JG03dev/CoSeL-lang.git"
  }
}
